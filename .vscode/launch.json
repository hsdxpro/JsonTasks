{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        // Debug
        {
            "name": "taraxa Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/taraxa",
            "preLaunchTask": "debug",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/bin/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        // Release with debug
        {
            "name": "kraken relwithdeb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/taraxa",
            "preLaunchTask": "relwithdeb",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/bin/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}